  src: url('/fonts/raleway/raleway-regular-webfont.eot?#iefix"') format('embedded-opentype'),
       url('/fonts/raleway/raleway-regular-webfont.woff') format('woff'),
       url('/fonts/raleway/raleway-regular-webfont.otf') format('opentype'),
       url('/fonts/raleway/raleway-regular-webfont.svg#raleway-regular') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Raleway3';
  src: url('/fonts/raleway/raleway-bold-webfont.eot');
  src: url('/fonts/raleway/raleway-bold-webfont.eot?#iefix"') format('embedded-opentype'),
       url('/fonts/raleway/raleway-bold-webfont.woff') format('woff'),
       url('/fonts/raleway/raleway-bold-webfont.otf') format('opentype'),
       url('/fonts/raleway/raleway-bold-webfont.svg#raleway-light') format('svg');
  font-weight: bold;
  font-style: normal;
}
